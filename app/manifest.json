{
    "name": "Pull Request Playbook",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Pull Request Playbook",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.github.com/*/pull*"
            ],
            "js": [
                "scripts/contentscript.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "permissions": [
      "tabs",
      "*://*.github.com/*"
    ]
}